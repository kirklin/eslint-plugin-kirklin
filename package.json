{
  "name": "eslint-plugin-kirklin",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@9.0.1",
  "description": "Kirk Lin extended ESLint rules",
  "author": "Kirk Lin (https://github.com/kirklin)",
  "license": "MIT",
  "funding": "https://www.buymeacoffee.com/linkirk",
  "homepage": "https://github.com/kirklin/eslint-plugin-kirklin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirklin/eslint-plugin-kirklin.git"
  },
  "bugs": "https://github.com/kirklin/eslint-plugin-kirklin/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "lint:fix": "pnpm run dev && eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@kirklin/eslint-config": "^2.3.0",
    "@types/eslint": "^8.56.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.12.7",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/rule-tester": "^7.7.0",
    "@typescript-eslint/typescript-estree": "^7.7.0",
    "@typescript-eslint/utils": "^7.7.0",
    "ajv": "^6.12.4",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "eslint-define-config": "^2.1.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "lodash.merge": "4.6.2",
    "pnpm": "^9.0.1",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "resolutions": {
    "eslint-plugin-kirklin": "workspace:*"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
